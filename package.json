{
  "name": "angular2-jsroot-socketio-zmq",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prestart": "npm install",
    "start": "concurrently \"npm run tsc:w\" \"node server.js\"",    
    "dev": "concurrently \"npm run tsc:w\" \"nodemon server.js\"",    
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings",
    "postinstall": "concurrently \"typings install\" \"bower install\"" 
  },
  "dependencies": {
    "js-beautify" : "*",
    "bower": "*",
    "express.io": "*",
    "connect": "*",
    "zmq":"*",
    "angular2": "2.0.0-beta.9",
    "systemjs": "0.19.24",
    "es6-promise": "^3.0.2",
    "es6-shim": "^0.33.3",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.2",
    "zone.js": "0.5.15"
  },
  "devDependencies": {
    "concurrently": "^2.0.0",
    "typescript": "^1.8.7",
    "typings":"^0.7.5",
    "nodemon": "^1.8.1"
  }
}
